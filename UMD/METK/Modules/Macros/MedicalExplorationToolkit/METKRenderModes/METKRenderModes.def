UserGenres {
   Genre METK {
      Genre = METKRenderModes {
         Title = "Render Modes"
      }
   }
}

MacroModule METKIsoSurface {
   author             = "Konrad Mühler, Christian Tietjen"
   status             = "stable"
   comment            = "Holds all needed data for iso surface visualization"
   keywords           = "METK shaded marching cubes"
   seeAlso            = "METKSilhouette METKTexturing"
   genre              = "METKRenderModes"
   externalDefinition = "$(LOCAL)/METKIsoSurface.script"
   exampleNetwork     = "$(LOCAL)/../ExampleNetworks/METKRenderModes.mlab"
   documentation      = "http://www.metk.net/METKDocumentation/METKIsoSurface.html"
}

MacroModule METKSilhouette {
   author             = "Christian Tietjen"
   status             = "stable"
   comment            = "Holds all needed data for silhouette visualization"
   keywords           = "METK llustrative NPR"
   seeAlso            = "METKStippling METKIsoSurface METKTexturing"
   genre              = "METKRenderModes"
   externalDefinition = "$(LOCAL)/METKSilhouette.script"
   exampleNetwork     = "$(LOCAL)/../ExampleNetworks/METKRenderModes.mlab"
   documentation      = "http://www.metk.net/METKDocumentation/METKSilhouette.html"
}

