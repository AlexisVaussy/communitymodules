MacroModule METKSaveCase {
   genre              = "METKOpen"
   author = "Konrad Mühler, Christian Tietjen"
   status = "stable"
   comment = "Helps you to save a case in the right directory. It also prompts a warning if you may lost changes."
   keywords = "save open dump "
   seeAlso = "METKManager ObjDump"   
   externalDefinition = "$(LOCAL)/METKSaveCase.script"
   exampleNetwork = "$(LOCAL)/../ExampleNetworks/METKSaveCase.mlab"
   documentation      = "http://www.metk.net/METKDocumentation/METKSaveCase.html"
}


