UserGenres {
   Genre METK {
      Genre = METKOpen {
         Title = "Loader"
      }
   }
}


MacroModule METKManager {
   author             = "Christian Tietjen, Konrad Mühler"
   status             = "work-in-progress"
   comment            = "Main module needed for every application using the METK"
   keywords           = "METK manager central container information ObjMgr"
   seeAlso            = ""
   genre              = "METK"
   externalDefinition = "$(LOCAL)/METKManager.script"
   exampleNetwork = "$(LOCAL)/../ExampleNetworks/METKRenderModes.mlab"
   documentation      = "http://www.metk.net/METKDocumentation/METKManager.html"
}


MacroModule METKPackFiles {
   author             = "Konrad Mühler"
   status             = "work-in-progress"
   comment            = "Convert (Pack) TIFF files in a directory"
   keywords           = "METK tiff dicom"
   seeAlso            = ""
   genre              = "METKOpen"
   externalDefinition = "$(LOCAL)/METKPackFiles.script"
   documentation      = "http://www.metk.net/METKDocumentation/METKPackFiles.html"
}


MacroModule METKConvertFiles {
