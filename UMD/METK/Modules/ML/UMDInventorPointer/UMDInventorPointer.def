MLModule UMDInventorToPointer {
  DLL            = "UMDInventorPointer"

  genre          = ""
  author         = "Konrad"
  status         = "stable"
  comment        = ""
  keywords       = ""
  seeAlso        = ""
  documentation  = "$(LOCAL)/html/UMDInventorToPointer.html"
  projectSource  = "$(UserProjectsPath)/sources/ML/UMDInventorPointer"
  exampleNetwork     = "$(LOCAL)/Example/Beam.mlab"
  Window UMDInventorToPointer {
     Vertical {
	    Field = objectID
	    Field = layerID
	    Field = infoID
	    Field = Pointer { edit = No w = 200 }
	 }
  }
} // MLModule UMDInventorToPointer



