MLModule U3DAddModelFromFiberSet {
  DLL                = "MLU3D"
  genre              = "U3DModules"
  author             = "Axel Newe"
  comment            = "Adds a model to a U3D."
  keywords           = "PDF U3D"
  seeAlso            = ""
  exampleNetwork     = "$(LOCAL)/networks/U3DExample.mlab"
  externalDefinition = $(LOCAL)/U3DAddModelFromFiberSet.script
}

