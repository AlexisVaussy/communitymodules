// Genres ----------------------------------------------------------------------

UserGenres {
  Genre PDF { 
    Genre U3DModules   { title = "U3D Modules" } 
  }
}

// Inspectors ---------------------------------------------------------------------

OutputInspector {
  type   = "MLBase.mlU3D::U3DObject"
  module = "U3DInspector"
  field  = "inU3DObject"
}

