MLModule IndexPairListContainer {
  genre              = Marker
  DLL                = MLBaseListExtensions
  author             = "Axel Newe"
  comment            = "Container for a IndexPair list object."
  keywords           = "Vector ListBase XMarker"
  seeAlso            = "StringListContainer SoView2DMarkerEditor XMarkerListContainer"
  externalDefinition = $(LOCAL)/IndexPairListContainer.script
}

MLModule ColoredMarkerListContainer {
  DLL                = MLBaseListExtensions
  genre              = "Marker"
  author             = "Axel Newe"
  comment            = "Container for a ColoredMarker list object."
  keywords           = "ListBase XMarker ColoredMarker"
  seeAlso            = "StringListContainer SoView2DMarkerEditor XMarkerListContainer"
  externalDefinition = $(LOCAL)/ColoredMarkerListContainer.script
}

MLModule XMarkerToColoredMarker {
  DLL                = MLBaseListExtensions
  genre              = "Marker"
  author             = "Axel Newe"
  comment            = "Converts XMarkerList to ColoredMarkerList"
  keywords           = "ListBase XMarker ColoredMarker"
  seeAlso            = "XMarkerListContainer ColoredMarkerListContainer"
  externalDefinition = $(LOCAL)/XMarkerToColoredMarker.script
  exampleNetwork     = $(LOCAL)/networks/XMarkerToColoredMarkerExample.mlab
}  

MLModule ColoredMarkerToXMarker {
  DLL                = MLBaseListExtensions
  genre              = "Marker"
  author             = "Axel Newe"
  comment            = "Converts ColoredMarkerList to XMarkerList"
  keywords           = "ListBase XMarker ColoredMarker"
  seeAlso            = "XMarkerListContainer ColoredMarkerListContainer"
  externalDefinition = $(LOCAL)/ColoredMarkerToXMarker.script
  exampleNetwork     = $(LOCAL)/networks/ColoredMarkerToXMarkerExample.mlab
}  

